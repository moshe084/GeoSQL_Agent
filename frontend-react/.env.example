# ==============================================================================
# Geo-SQL Agent - Frontend React Configuration
# ==============================================================================
# Copy this file to .env for local development
# Environment variables must start with REACT_APP_ to be accessible in React

# ------------------------------------------------------------------------------
# API Configuration
# ------------------------------------------------------------------------------
# Backend API URL - change this based on your environment
REACT_APP_API_URL=http://localhost:8000

# ------------------------------------------------------------------------------
# Map Configuration
# ------------------------------------------------------------------------------
# Default map center coordinates [latitude, longitude]
# Tel Aviv coordinates as default
REACT_APP_MAP_CENTER_LAT=32.0853
REACT_APP_MAP_CENTER_LNG=34.7818
REACT_APP_MAP_DEFAULT_ZOOM=13

# ------------------------------------------------------------------------------
# Application Settings
# ------------------------------------------------------------------------------
REACT_APP_NAME=Geo-SQL Agent
REACT_APP_VERSION=1.0.0

# ------------------------------------------------------------------------------
# Feature Flags
# ------------------------------------------------------------------------------
REACT_APP_ENABLE_QUERY_HISTORY=true
REACT_APP_MAX_HISTORY_ITEMS=10
